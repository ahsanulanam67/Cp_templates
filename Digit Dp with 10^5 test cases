mt19937 rng(chrono::steady_clock::now().time_since_epoch().count());
int my_rand(int l, int r)
{
    return uniform_int_distribution<int>(l, r) (rng);
}

ll dp[51][51][51][51][2];
string s;
ll gdp(ll in, ll digitsum, ll divisibility, ll m,ll flag)
{

    //cout<<digitsum<<"  string "<<s<<"  "<<divisibility<<endl;
    if(in>=s.size())
    {


        if(digitsum%m == 0 && divisibility%m==0)
        {
           // cout<<digitsum<<" vitorestirng "<<s<<"  "<<divisibility<<endl;
            return 1;
        }
        return 0;
    }

    auto &an = dp[(s.size()) - in-1][digitsum][divisibility][m][flag];

    if(~an && flag == 0)
    {
        return an;
    }
    an = 0;
   // cout<<flag<<" flagg"<<endl;
    if(flag==1)
    {

        int  x = s[in] - '0';

        for(int i = 0; i<=x; i++)
        {
            an += gdp(in+1, (digitsum+i)%m, (divisibility*10 + i)%m, m, i==x?1:0);
            an %= mod;
        }

    }
    else
    {

        for(int i = 0; i<=9; i++)
        {


            an += gdp(in+1, (digitsum+i)%m, (divisibility*10 + i)%m, m, flag);
            an%= mod;
        }
    }


    return an%mod;



}
bool check(string s,ll m){

    ll x= 0,y=0;

    for(int i=0;i<s.size();i++){

        x = x*10 + (s[i]-'0');
        x%=m;
        y+=(s[i]-'0');
        y%=m;
    }

    return x==0 && y==0;

}

void akam()
{


    string l,r;
    ll k;
    cin>>l>>r>>k;
    s =  r;
    ll x = gdp(0,0,0,k,1);
    s = l;
   // cout<<x<<" XX"<<endl;
    ll y = gdp(0,0,0,k,1);
    ll ans = (x-y+mod)%mod;
   // cout<<y<<" yy"<<endl;

    if(check(l,k)==1){ans++;ans%=mod;}
    cout<<ans<<endl;

}
